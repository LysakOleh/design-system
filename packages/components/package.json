  "module": "dist/index.js",
  "es2015": "dist/esm/index.esm.js",
  "es2017": "dist/esm/index.esm.js",
  "types": "dist/types/index.d.ts",
  "collection": "dist/collection/collection-manifest.json",
  "collection:main": "dist/collection/index.js",
  "unpkg": "dist/design-system-components/design-system-components.esm.js",
  "files": [
    "dist/",
    "loader/",
    "src/styles/",
    "src/components/**/*.sass",
    "src/components/**/*.scss"
  ],
  "scripts": {
    "start": "npm run dev",
    "update:registry": "sed -i -e \"s#nexus.balgroupit.com/repository/npm#registry.npmjs.org#g\" package-lock.json\n",
    "==================== BUILD ====================": "================================",
    "build": "npm run build:meta && npm run build:testing && stencil build",
    "build:meta": "node .build/contributors.js && node .build/github.js",
    "build:testing": "node .build/testing.js",
    "build:docs": "stencil build --config stencil.docs.config.ts && build-storybook -o ../../storybook  --quiet",
    "================= DEVELOPMENT =================": "================================",
    "generate": "node ./.build/stencil/generate",
    "dev": "npm run build:meta && npm run dev:build && concurrently --names \"ðŸ§©_STENCIL__,ðŸ“•_STORYBOOK\" -c \"blue,magenta\" \"npm:dev:watch\" \"npm:dev:docs\"",
    "dev:build": "stencil build --config stencil.pre.dev.config.ts",
    "dev:watch": "stencil build --config stencil.dev.config.ts --watch",
    "dev:docs": "start-storybook -p 6006 --quiet",
    "==================== TEST =====================": "================================",
    "test": "stencil test --spec --config stencil.test.config.ts",
    "test:watch": "stencil test --spec --watchAll --config stencil.test.config.ts",
    "log": "stencil build --config stencil.log.config.ts",
    "www": "stencil build --config stencil.test.config.ts",
    "serve:dev": "stencil build --config stencil.test.config.ts --dev --serve --watch",
    "serve": "stencil build --config stencil.test.config.ts --prod --serve --watch",
    "cy:build": "npm run www",
    "cy:start": "node ./web-server.js",
    "cy:install": "cypress install",
    "cy:run": "cypress run --browser chrome --config excludeSpecPattern=\"cypress/e2e/visual/*cy.ts\"",
    "cy:open": "cypress open --browser chrome --config excludeSpecPattern=\"cypress/e2e/visual/*cy.ts\"",
    "cy:component:run": "cypress run --browser chrome --component --config excludeSpecPattern=\"cypress/e2e/visual/*cy.ts\"",
    "cy:visual:base": "cypress run --browser chrome --env type=base --spec \"cypress/e2e/visual/*cy.ts\" --config screenshotsFolder=cypress/snapshots/base",
    "cy:visual:actual": "cypress run --browser chrome --env type=actual --spec \"cypress/e2e/visual/*cy.ts\""
  },
